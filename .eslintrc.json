{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["allure-report/**"],
    "rules": {
        "max-len": ["error", { "code": 200, "ignoreComments": true, "ignoreUrls": true }],
        "require-jsdoc": "off",
        "valid-jsdoc": "off"
    }
}

